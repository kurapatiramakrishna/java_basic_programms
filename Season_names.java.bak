import java.util.Scanner;
class Season_names
{
	public static void main(String[] args) 
	{
     Scanner scan=new Scanner(System.in);
	 System.out.println("To check the check the season:"); 
	 System.out.println("Enter the month and to find the season:");
	int month=scan.nextInt();

		if (month==3||month==4||month==5)
		{
			System.out.println("The season is SUMMER :");
		}
		else if (month==6||month==7||month==8||month==9||month==10||month==11)
		{
			System.out.println("The season is RINY :");
		}
		else if (month==12||month==1||month==2)
		{
			System.out.println("The season is WINTER. :");
		}
		else
		{
			System.out.println("The valid season that you want to print :");
		}
}
}
