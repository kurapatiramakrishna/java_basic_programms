import java.util.Scanner;
class Vowels_checking
{
	public static void main(String[] args) 
	{
     Scanner scan=new Scanner(System.in);
	 System.out.println("To check the given alphabet is vowel or not:");
	 System.out.println("Enter the alphabet and you want to print:");
	 char alp=scan.next().charAt(0);

		if (alp=='A'||alp=='E'||alp=='I'||alp=='o'||alp=='U'||alp=='a'||alp=='e'||alp=='i'||alp=='o'||alp=='u')
		{
			System.out.println("The given later is vowel latter :");
		}
		else
		{
			System.out.println("the given later is not a vowel latter:");
		}
	}
}
